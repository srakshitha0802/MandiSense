# MandiSense 2.0 - Environment Variables Example
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_BASE_URL=https://api.mandisense.com
REACT_APP_API_VERSION=v1

# External Service APIs
REACT_APP_WEATHER_API_KEY=your_weather_api_key_here
REACT_APP_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_VOICE_API_KEY=your_voice_recognition_api_key_here

# Blockchain Configuration
REACT_APP_BLOCKCHAIN_NETWORK=mainnet
REACT_APP_BLOCKCHAIN_RPC_URL=https://mainnet.infura.io/v3/your_project_id
REACT_APP_CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890

# Feature Flags
REACT_APP_ENABLE_VOICE_FEATURES=true
REACT_APP_ENABLE_BLOCKCHAIN=true
REACT_APP_ENABLE_IOT=true
REACT_APP_ENABLE_AI_FEATURES=true

# Analytics and Monitoring
REACT_APP_ANALYTICS_ID=your_analytics_id
REACT_APP_SENTRY_DSN=your_sentry_dsn_for_error_tracking

# Development Settings
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info

# Regional Settings
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_DEFAULT_CURRENCY=INR
REACT_APP_DEFAULT_TIMEZONE=Asia/Kolkata

# Security Settings
REACT_APP_ENABLE_HTTPS_ONLY=true
REACT_APP_SESSION_TIMEOUT=3600

# Performance Settings
REACT_APP_CACHE_DURATION=300
REACT_APP_MAX_FILE_SIZE=5242880

# Notification Services
REACT_APP_PUSH_NOTIFICATION_KEY=your_push_notification_key
REACT_APP_SMS_SERVICE_KEY=your_sms_service_key
REACT_APP_EMAIL_SERVICE_KEY=your_email_service_key

# Social Media Integration (Optional)
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id
REACT_APP_TWITTER_API_KEY=your_twitter_api_key
REACT_APP_WHATSAPP_BUSINESS_ID=your_whatsapp_business_id

# Development URLs
REACT_APP_DEV_API_URL=http://localhost:3001
REACT_APP_STAGING_API_URL=https://staging-api.mandisense.com
REACT_APP_PROD_API_URL=https://api.mandisense.com

# Database Configuration (if using backend)
REACT_APP_DB_HOST=localhost
REACT_APP_DB_PORT=5432
REACT_APP_DB_NAME=mandisense
REACT_APP_DB_USER=mandisense_user

# Redis Configuration (for caching)
REACT_APP_REDIS_HOST=localhost
REACT_APP_REDIS_PORT=6379

# File Storage
REACT_APP_STORAGE_BUCKET=mandisense-storage
REACT_APP_CDN_URL=https://cdn.mandisense.com

# Third-party Integrations
REACT_APP_PAYMENT_GATEWAY_KEY=your_payment_gateway_key
REACT_APP_LOGISTICS_API_KEY=your_logistics_api_key
REACT_APP_BANK_API_KEY=your_banking_api_key

# Machine Learning Services
REACT_APP_ML_API_URL=https://ml.mandisense.com
REACT_APP_IMAGE_RECOGNITION_KEY=your_image_recognition_key
REACT_APP_NLP_SERVICE_KEY=your_nlp_service_key

# IoT Configuration
REACT_APP_IOT_MQTT_BROKER=mqtt://iot.mandisense.com
REACT_APP_IOT_DEVICE_KEY=your_iot_device_key
REACT_APP_SENSOR_DATA_URL=https://sensors.mandisense.com

# Backup and Recovery
REACT_APP_BACKUP_SERVICE_URL=https://backup.mandisense.com
REACT_APP_RECOVERY_KEY=your_recovery_key

# Compliance and Legal
REACT_APP_PRIVACY_POLICY_URL=https://mandisense.com/privacy
REACT_APP_TERMS_OF_SERVICE_URL=https://mandisense.com/terms
REACT_APP_GDPR_COMPLIANCE=true

# Support and Help
REACT_APP_SUPPORT_EMAIL=support@mandisense.com
REACT_APP_HELP_DESK_URL=https://help.mandisense.com
REACT_APP_COMMUNITY_URL=https://community.mandisense.com

# Version Information
REACT_APP_VERSION=2.0.0
REACT_APP_BUILD_NUMBER=1
REACT_APP_RELEASE_DATE=2024-01-01

# Custom Branding (Optional)
REACT_APP_BRAND_NAME=MandiSense
REACT_APP_BRAND_LOGO_URL=https://cdn.mandisense.com/logo.png
REACT_APP_BRAND_COLOR=#22c55e
REACT_APP_BRAND_FONT=Inter

# Experimental Features
REACT_APP_ENABLE_BETA_FEATURES=false
REACT_APP_ENABLE_EXPERIMENTAL_UI=false
REACT_APP_ENABLE_A_B_TESTING=false

# Notes:
# - Never commit the actual .env file with real API keys
# - Keep sensitive information secure
# - Use different values for development, staging, and production
# - Some features may require specific API keys to function properly
# - Contact the development team for production API keys